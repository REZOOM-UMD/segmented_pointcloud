<launch>
   
   <node name="extract" pkg="image_view" type="extract_images" respawn="false" required="true" output="screen" cwd="node">
      <remap from="image" to="/zed2i/zed_node/rgb/image_rect_color"/>
      <param name="sec_per_frame" value="0.05"/>
      <param name="filename_format" value="/home/sharmitha/bag_images/1/%i.jpg"/>
      
   </node>

   <node name="pcd" pkg="pcl_ros" type="pointcloud_to_pcd" respawn="false" required="true" output="screen" cwd="node">
      <param name = "input:" value="/zed2i/zed_node/point_cloud/cloud_registered"/>
      <param name="filename_format" value="/home/sharmitha/bag_pcl/1/%i.pcd"/>
      
   </node>

   <node name="segmentation" pkg="jetson-inference" type="./segnet" output="screen">
      <param name="network" value="fcn-resnet18-citscapes"/>
      <param name="visualize" value="mask"/>
      <param name="filename_format" value=" "/>
      <param name="filename_format" value=" "/>
   </node>
   <node name="process_pcd" pkg="segmented_pointclod" type="pcl_array.py" output="screen">
      
      <param name="filename_format" value=" "/>
   </node>
   <node name="rostopic_node" pkg="rostopic" type="rostopic" output = "screen" args = "pub/new_cloud sensor_msgs/Point_cloud2" data:"" />
   

</launch>
